    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-group-selectors": "^1.0.8",
    "xo": "^0.48.0"
  }
